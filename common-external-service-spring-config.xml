<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<context:component-scan
		base-package="gov.usda.fsa.fcao.flp.flpids.common" />

	<bean id="flpOfficeMRTBusinessService"
		class="gov.usda.fsa.citso.cbs.client.OfficeDataServiceProxy" />

	<bean id="surrogateService"
		class="gov.usda.fsa.citso.cbs.client.TaxIdSurrogateBusinessServiceProxy" />

	<bean id="businessPartyDataService"
		class="gov.usda.fsa.citso.cbs.client.BusinessPartyDataServiceProxy" />

	<bean id="employeeDataService"
		class="gov.usda.fsa.citso.cbs.client.EmployeeDataServiceProxy" />

	<bean id="flpStateMRTBusinessService"
		class="gov.usda.fsa.citso.cbs.client.StateDataServiceProxy" />

	<bean id="flpInterestRateMRTBusinessService"
		class="gov.usda.fsa.citso.cbs.client.InterestRateDataServiceProxy" />

	<bean id="flpLocationAreaDataMartBusinessService"
		class="gov.usda.fsa.citso.cbs.client.LocationAreaDataServiceProxy" />

	<bean id="cbsCountyDataServiceProxy"
		class="gov.usda.fsa.citso.cbs.client.CountyDataServiceProxy" />

	<bean id="cbsStateDataServiceProxy"
		class="gov.usda.fsa.citso.cbs.client.StateDataServiceProxy" />

	<bean id="calendarDataServiceProxy"
		class="gov.usda.fsa.citso.cbs.client.CalendarDataServiceProxy" />

	<bean id="mrtFacadeBusinessService"
		class="gov.usda.fsa.fcao.flp.flpids.common.business.businessServices.MRTFacadeBusinessServiceImpl">
		<property name="officeBusinessService"
			ref="flpOfficeMRTBusinessService" />
		<property name="stateBusinessService"
			ref="flpStateMRTBusinessService" />
		<property name="locationBusinessService"
			ref="flpLocationAreaDataMartBusinessService" />
	</bean>
	<bean id="mrtProxyBS"
		class="gov.usda.fsa.fcao.flp.flpids.common.business.businessServices.MRTProxyBS">
		<property name="flpOfficeMRTBusinessService"
			ref="flpOfficeMRTBusinessService" />
		<property name="flpStateMRTBusinessService"
			ref="flpStateMRTBusinessService" />
		<property name="interestRateDataMartBusinessService"
			ref="flpInterestRateMRTBusinessService" />
		<property name="flpLocationAreaDataMartBusinessService"
			ref="flpLocationAreaDataMartBusinessService" />
		<property name="businessPartyDataService"
			ref="businessPartyDataService" />
		<property name="employeeDataServiceProxy"
			ref="employeeDataService" />
		<property name="mrtFacadeBusinessService"
			ref="mrtFacadeBusinessService" />
		<property name="surrogateService" ref="surrogateService" />
		<property name="countyDataServiceProxy"
			ref="cbsCountyDataServiceProxy" />
		<property name="calendarDataServiceProxy"
			ref="calendarDataServiceProxy" />
	</bean>

	<bean id="mrtStateAbbrFromStateFLPLookUp"
		class="gov.usda.fsa.fcao.flp.flpids.common.business.businessServices.MRTStateAbbrFromStateFLPLookUp">
		<property name="mrtProxyBusinessService" ref="mrtProxyBS" />
	</bean>
	<bean id="serviceCenterFLPCodeManager"
		class="gov.usda.fsa.fcao.flp.flpids.common.business.businessServices.ServiceCenterFLPCodeManagerImpl">
		<property name="mrtProxyBusinessService" ref="mrtProxyBS" />
		<property name="stateAbbrFromStateFLPLookUp"
			ref="mrtStateAbbrFromStateFLPLookUp" />
	</bean>

	<bean id="normalizeAreaOfResponsibility"
		class="gov.usda.fsa.fcao.flp.flpids.common.utilities.NormalizeAreaOfResponsibility" />

	<bean id="easUserProfile"
		class="gov.usda.fsa.fcao.flp.flpids.common.auth.EASUserProfile">
		<property name="normalizeAreaOfResponsibility"
			ref="normalizeAreaOfResponsibility" />
	</bean>
	<bean id="easNatsUserProfile"
		class="gov.usda.fsa.fcao.flp.flpids.common.auth.EASNatsUserProfile">
		<property name="normalizeAreaOfResponsibility"
			ref="normalizeAreaOfResponsibility" />
	</bean>

	<bean id="contractValidator"
		class="gov.usda.fsa.fcao.flp.flpids.common.ContractValidator" />

	<bean id="mockUserProfile"
		class="gov.usda.fsa.fcao.flp.flpids.common.auth.MockUserProfile">
		<property name="normalizeAreaOfResponsibility"
			ref="normalizeAreaOfResponsibility" />
		<property name="contractValidator" ref="contractValidator" />
	</bean>

	<bean id="interestRateBusinessService"
		class="gov.usda.fsa.fcao.flp.services.interest.impl.MartInterestRateBusinessServiceImpl">
		<property name="dataMartBusinessService" ref="mrtProxyBS" />
	</bean>

	<bean id="userInformationService"
		class="gov.usda.fsa.fcao.flp.services.user.impl.UserInformationBusinessServiceImpl">
		<property name="dataMartBusinessService" ref="mrtProxyBS" />
	</bean>

</beans>